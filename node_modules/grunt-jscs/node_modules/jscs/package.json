  "devDependencies": {
    "chai": "^3.3.0",
    "coveralls": "~2.11.2",
    "has-ansi": "~2.0.0",
    "jshint": "~2.8.0",
    "mocha": "^2.2.0",
    "regenerate": "~1.2.1",
    "rewire": "^2.3.1",
    "sinon": "^1.13.0",
    "sinon-chai": "^2.8.0",
    "unicode-7.0.0": "~0.1.5",
    "unit-coverage": "^4.0.1",
    "xml2js": "~0.4.4"
  },
  "bin": {
    "jscs": "./bin/jscs"
  },
  "unit-coverage": {
    "common": [
      "-a",
      "lib",
      "-a",
      "test",
      "-s",
      "lib/**/*.js",
      "-t",
      "test/specs/**/*.js",
      "-e",
      "lib/cli-config.js",
      "-S",
      "relative",
      "-O",
      "sources=lib",
      "-O",
      "tests=test/specs"
    ]
  },
  "scripts": {
    "lint": "npm run jshint && npm run jscs",
    "jshint": "jshint .",
    "jscs": "node bin/jscs lib test bin publish",
    "pretest": "npm run lint",
    "test": "mocha --color",
    "watch": "mocha --color --watch",
    "integration": "#node test/scripts/integration.js",
    "coverage": "unit-coverage run -p common",
    "coverage-html": "unit-coverage run -p common -r html -o coverage.html",
    "coveralls": "unit-coverage run -p common -r lcov -o out.lcov && cat out.lcov | coveralls",
    "changelog": "git log `git describe --tags --abbrev=0`..HEAD --pretty=format:' * %s (%an)' | grep -v 'Merge pull request'",
    "release": "node publish/prepublish && npm test && npm publish"
  },
  "files": [
    "bin",
    "lib",
    "patterns",
    "presets",
    "LICENSE"
  ],
  "readme": "# JSCS has [merged](https://medium.com/@markelog/jscs-end-of-the-line-bc9bf0b3fdb2#.x0o1xqkk9) with ESLint!\n\n[![NPM Version](https://img.shields.io/npm/v/jscs.svg?style=flat)](https://www.npmjs.com/package/jscs)\n[![Build Status](https://travis-ci.org/jscs-dev/node-jscs.svg?branch=master)](https://travis-ci.org/jscs-dev/node-jscs)\n[![Coverage Status](https://img.shields.io/coveralls/jscs-dev/node-jscs.svg?style=flat)](https://coveralls.io/r/jscs-dev/node-jscs?branch=master)\n\n**JSCS** is a code style linter and formatter for your style guide\n\n<!-- intro-end -->\n\n* [Tutorial and Overview](http://jscs.info/overview)\n* [Codestyle Rules](http://jscs.info/rules)\n* [How to Contribute](http://jscs.info/contributing)\n",
  "readmeFilename": "README.md",
  "_id": "jscs@3.0.7",
  "_from": "jscs@~3.0.5"
}
