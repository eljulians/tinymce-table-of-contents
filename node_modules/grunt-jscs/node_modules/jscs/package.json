    "chalk": "~1.1.0",
    "cli-table": "~0.3.1",
    "commander": "~2.9.0",
    "cst": "^0.4.3",
    "estraverse": "^4.1.0",
    "exit": "~0.1.2",
    "glob": "^5.0.1",
    "htmlparser2": "3.8.3",
    "js-yaml": "~3.4.0",
    "jscs-jsdoc": "^2.0.0",
    "jscs-preset-wikimedia": "~1.0.0",
    "jsonlint": "~1.6.2",
    "lodash": "~3.10.0",
    "minimatch": "~3.0.0",
    "natural-compare": "~1.2.2",
    "pathval": "~0.1.1",
    "prompt": "~0.2.14",
    "reserved-words": "^0.1.1",
    "resolve": "^1.1.6",
    "strip-bom": "^2.0.0",
    "strip-json-comments": "~1.0.2",
    "to-double-quotes": "^2.0.0",
    "to-single-quotes": "^2.0.0",
    "vow": "~0.4.8",
    "vow-fs": "~0.3.4",
